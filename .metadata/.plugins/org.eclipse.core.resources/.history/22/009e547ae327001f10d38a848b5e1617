package Java8;

interface Drawable
{
	public void draw();
}


public class LambdaExpressionExample{

	
	public static void main(String[] args) {
		int width = 10;
		 
		LambdaExpressionExample l = new LambdaExpressionExample();
		l.draw();
	}

	@Override
	public void draw() {
		System.out.println("lavda lasun");
		
	}

	
	
	
}
